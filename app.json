{
  "expo": {
    "name": "Serendipia",
    "slug": "seren",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon.png",
    "userInterfaceStyle": "light",
    "scheme": "seren",
    "splash": {
      "image": "./assets/newLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
